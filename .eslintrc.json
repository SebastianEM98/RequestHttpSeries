{
    "parserOptions":{
        "ecmaVersion": 2018
    }, 
    "extends": ["eslint: recommended", "prettier"],
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "no-var": "error",
        "no-multi-spaces": "error",
        "no-multiple-empty-lines": "error",
        "prefer-const": "error",
        "no-console": "warn"
    }
}